<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ylj.dao.UserMapper">
    <select id="selectAll" resultType="user">
        select * from user
    </select>

    <select id="insert" parameterType="cn.ylj.entity.UserEntity">
        insert into user values (#{id},#{username},#{password},#{gender},#{age},#{address},#{qq},#{email})
    </select>

    <update id="update" parameterType="cn.ylj.entity.UserEntity">
        update user set username = #{username}, address=#{address} where id = #{id}
    </update>

    <delete id="delete" parameterType="int">
        delete from user where id = #{id}
    </delete>
</mapper>